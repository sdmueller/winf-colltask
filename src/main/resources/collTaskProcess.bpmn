    </bpmn2:endEvent>
    <bpmn2:sequenceFlow id="SequenceFlow_9" name="" sourceRef="ExclusiveGateway_3" targetRef="EndEvent_2">
      <bpmn2:conditionExpression xsi:type="bpmn2:tFormalExpression"><![CDATA[${chooseAction.equals("finished")}]]></bpmn2:conditionExpression>
    </bpmn2:sequenceFlow>
